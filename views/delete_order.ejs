<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="../assests/styles.css">
</head>
<body>
    <h4>Click the order that you want to delete.</h4>
<% for(var i =0;i<orders.length;i++){ %>
    <form action="/odelete" method="post">
      <div class="Order">
        <input type="checkbox" name="checkbox" value="<%=orders[i]._id%>" onChange="this.form.submit()">
        <p><%=orders[i].title%> order placed at <%=orders[i].time%> with total price of <%=orders[i].price%>.</p>
      </div>
    </form>
  <% }     %>
</body>
</html>
